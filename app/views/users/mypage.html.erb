<p style="color: green"><%= notice %></p>

<h1><%= current_user.name %>'s mypage</h1>

<div id="mypage.votings">
<table border=1>
  <tr>
    <th>Title</th>
    <th>deadline</th>
  </tr>
  <% current_user.votings.each do |voting|%>
    <tr>
      <th><%= link_to voting.title, voting_path(voting) %></th>
      <td><%= voting.deadline %></td>
    </tr>
  <% end %>
</table>

<%= link_to "Create new voting", new_voting_path %>
</div>

<%= link_to "Sign out", destroy_user_session_path, data: { turbo_method: :delete } %>